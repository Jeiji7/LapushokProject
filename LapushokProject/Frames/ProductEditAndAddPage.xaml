<Window x:Class="LapushokProject.Frames.ProductEditAndAddPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LapushokProject.Frames"
        mc:Ignorable="d"
        Title="SettingProduct" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="80*"/>
        </Grid.RowDefinitions>
        <TextBlock Style="{DynamicResource 'H1 '}">Добавление продукта</TextBlock>

        <StackPanel Grid.Row="1" Orientation="Vertical">
            <TextBlock>Название</TextBlock>
        </StackPanel>
    </Grid>
</Window>
